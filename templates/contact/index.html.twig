{% extends 'base.html.twig' %}

{% block title %}contact{% endblock %}

{% block body %}
    <main class="container">
        <section id="contact">
            <div class="justify-content-center col-12 row">
                <div  class="col-md-6 col-12">
                    <img src="{{ asset('img/logo/contact-img.png') }}" style="max-width:100%;"  alt="">
                </div>
                <div class="col-md-6 col-12">
                    <div class="card">
                        <div class="card-body">
                            {{ form(form) }}
                        </div>
                    </div>
                </div>
            </div>
        </section>
    </main>
{% endblock %}
